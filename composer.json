{
    "name": "sparrow-team/laravel-setting-module",
    "description": "Customizable setting module for laravel framework developed by sparrow team",
    "license": "MIT",
    "authors": [
        {
            "name": "Sparrow team",
            "email": "kpouriya6@gmail.com"
        }
